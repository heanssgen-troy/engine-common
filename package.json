{
  "name": "@heanssgen-troy/engine-common",
  "version": "1.0.2",
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": "https://github.com/heanssgen-troy/engine-common.git",
  "scripts": {
    "generate": "npm run generate-proto && npm run generate-types && npm run generate-barrels",
